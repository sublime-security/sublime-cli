{%- if verbose %}
{%- for result in results %}
<bold>DETECTION {{ loop.index }}</bold>
{%- if result.org_detection.active %}
<key>Status: </key><green>active</green>
{%- else %}
<key>Status: </key><light-red>inactive</light-red>
{%- endif %}
{%- if result.name %}
<key>Name: </key><light-yellow>{{ result.name }}</light-yellow>
{%- endif %}
<key>ID: </key><dim>{{ result.id }}</dim>
<key>Created: </key><dim>{{ result.created_at }}</dim>
<key>Updated: </key><dim>{{ result.updated_at }}</dim>
<key>Org name: </key><dim>{{ result.created_by_org_name }}</dim>
<key>Org ID: </key><dim>{{ result.created_by_org_id }}</dim>
<key>Sublime user name: </key><dim>{{ result.created_by_sublime_user_full_name }}</dim>
<key>Sublime user ID: </key><dim>{{ result.created_by_sublime_user_id }}</dim>
